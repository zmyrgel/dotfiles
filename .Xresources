! -----------------------------------------
! Ratmen options
ratmen.style:            snazzy
ratmen.align:             left
ratmen.background:         grey22
ratmen.foreground:         darkorange
ratmen.font:            tamsyn-16
ratmen.mouse:             false
ratmen.scrollOffset:         5
ratmen.unfocusExit:         true

! -----------------------------------------
! Rofi resources
rofi.font:		tamsyn 16
rofi.kb-cancel:		Escape,Control+bracketleft,Control+g
rofi.terminal:		xterm
rofi.color-normal:	#fdf6e3,#002b36,#eee8d5,#586e75,#eee8d5
rofi.color-urgent:	#fdf6e3,#dc322f,#eee8d5,#dc322f,#fdf6e3
rofi.color-active:	#fdf6e3,#268bd2,#eee8d5,#268bd2,#fdf6e3
rofi.color-window:	#fdf6e3,#002b36
rofi.separator-style:	dash

! -----------------------------------------
! Emacs resources
!Emacs.background:               #414141
Emacs.menuBar:			off
Emacs.verticalScrollBars:	off
Emacs.toolBar:			off
!Emacs.font:                     input

! Dracula Xresources palette
*.foreground: #F8F8F2
*.background: #282A36
*.color0:     #000000
*.color8:     #4D4D4D
*.color1:     #FF5555
*.color9:     #FF6E67
*.color2:     #50FA7B
*.color10:    #5AF78E
*.color3:     #F1FA8C
*.color11:    #F4F99D
*.color4:     #BD93F9
*.color12:    #CAA9FA
*.color5:     #FF79C6
*.color13:    #FF92D0
*.color6:     #8BE9FD
*.color14:    #9AEDFE
*.color7:     #BFBFBF
*.color15:    #E6E6E6

! -----------------------------------------
! Xresources for applications not supporting fontconfig
Xft.autohint:	0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintfull
Xft.hinting:	1
Xft.antialias:	1
Xft.rgba:	rgb
Xft.dpi:	96

! -----------------------------------------
! Xcursor
Xcursor.theme: OpenZone
Xcursor.size:  22

! -----------------------------------------
! Dzen2 settings
dzen2.font:       -*-fixed-*-*-*-*-*-*-*-*-*-*-*-*
dzen2.foreground: #bebebe
dzen2.background: #373737

! -----------------------------------------
! Xterm setting
xterm*termName: 	xterm-256color
!xterm*faceName:		tamsyn-16
!xterm*faceName:		spleen-16
!xterm*faceName:		gohufont-16
!xterm*faceName:		'Input-10'
xterm*faceName: Input Mono
xterm*faceSize: 12
xterm*renderFont: true
xterm*scrollBar: 	false
xterm*fullscreen: 	never
xterm*loginShell:	true
xterm*saveLines:	4096
xterm*bellIsUrgent:	true
xterm*borderWidth:	0
xterm*multiClickTime:	250
xterm*backarrowKey:     false
xterm*ttyModes:         erase ^?
xterm*eightBitInput:	false
xterm*metaSendsEscape:	true
xterm*cursorUnderLine:	true
!xterm*background:	#414141
!xterm*background:	#373737
!xterm*foreground:	#bebebe
xterm*cursorColor:	#aaaaaa

! xlock ----------------------------------------------------------------------
xpdf*fontList:		-*-terminus-medium-r-normal--14-*-*-*-*-*-iso8859-1
xpdf.initialZoom:	width

! x11-ssh-askpass ------------------------------------------------------------
ssh-askpass*font:                   tamsyn-16
ssh-askpass*background:             #414141
ssh-askpass*foreground:             #bebebe
ssh-askpass.Button*background:      #373737
ssh-askpass.Indicator*foreground:   #bebebe
ssh-askpass.Indicator*background:   #373737
ssh-askpass*topShadowColor:         #000000
ssh-askpass*bottomShadowColor:      #000000
ssh-askpass.*borderWidth:           1

! xidle ----------------------------------------------------------------------
XIdle.timeout: 300

! xlock ----------------------------------------------------------------------
XLock*font:             -misc-spleen-medium-r-normal--16-160-72-72-c-80-iso10646-1
XLock*planfont:             -misc-spleen-medium-r-normal--16-160-72-72-c-80-iso10646-1
!XLock.font: 		-*-terminus-medium-r-normal--24-*-*-*-*-*-iso8859-1
!XLock.planfont: 	-*-terminus-medium-r-normal--24-*-*-*-*-*-iso8859-1
!XLock.mode:		blank
!XLock.blank.delay:	2000000
!XLock.grabmouse:	on
!XLock.mousemotion:	on
!XLock.usefirst:		yes
!XLock.lockdelay:	10
!XLock.nice:		19

! XMessage -------------------------------------------------------------------
!Xmessage*font:                  -*-terminus-medium-r-*-*-14-*-*-*-*-*-*-*
!Xmessage*font:             -misc-spleen-medium-r-normal--16-160-72-72-c-80-iso10646-1
Xmessage*font:             -misc-spleen-medium-r-normal--16-160-72-72-c-80-iso10646-1
Xmessage*background:            #242424
Xmessage*foreground:            #CCCCCC
Xmessage*form.*.shapeStyle:     rectangle
Xmessage*Scrollbar.width:       1
Xmessage*Scrollbar.borderWidth: 0
!Xmessage.form.message.Scroll:  WhenNeeded
Xmessage*Buttons:               Quit
Xmessage*defaultButton:         Quit
!Xmessage*geometry:             +20+20
